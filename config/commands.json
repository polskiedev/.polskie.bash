{
    "commands": [
        {
            "command": "list",
            "alias": "ls",
            "description": "List files"
        },
        {
            "command": "cd:home",
            "alias": "cd $HOME",
            "description": "Go to Home Directory"
        },
        {
            "command": "lc",
            "alias": "strtolower",
            "description": "Convert string to lowercase"
        },
        {
            "command": "make:note",
            "alias": "make_note",
            "description": "Make note"
        },
        {
            "command": "note",
            "alias": "write_note",
            "description": "Write note"
        },
        {
            "command": "install:vscode.ext",
            "alias": ". $PATH_DEVAPPS/setup.sh install:vscode.ext",
            "description": "Install Vscode extensions"
        },
        {
            "command": "backup:vscode.ext",
            "alias": ". $PATH_DEVAPPS/setup.sh backup:vscode.ext",
            "description": "Backup list of Vscode extensions"
        }
    ]
}

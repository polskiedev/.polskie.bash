{
    "commands": [{
            "command": "cd:home",
            "alias": "cd $HOME",
            "description": "Go to Home Directory"
        },
        {
            "command": "note",
            "alias": "write_note",
            "description": "Add a note for today if one doesn't already exist, or edit the existing note if it does."
        },
        {
            "command": "note:l",
            "alias": "list_notes",
            "description": "List of available notes"
        },
        {
            "command": "vsce:i",
            "alias": ". $PATH_DEVAPPS/setup.sh install:vscode.ext",
            "description": "Install Vscode extensions"
        },
        {
            "command": "vsce:bak",
            "alias": ". $PATH_DEVAPPS/setup.sh backup:vscode.ext",
            "description": "Backup list of Vscode extensions"
        },
        {
            "command": "gitignore",
            "alias": "add_gitignore",
            "description": "Copy gitignore from git dotfiles"
        },
        {
            "command": "autoload",
            "alias": "open_autoload_files",
            "description": "Open autoload files for editing"
        },
        {
            "command": "deploy",
            "alias": "devenv_deploy",
            "description": "Deploy devenv repository"
        }
    ]
}
{
    "commands": [
        {
            "command": "deploy",
            "alias": "devenv_deploy; set_ps",
            "group": "DevEnv",
            "description": "Deploy devenv repository"
        },
        {
            "command": "note:create",
            "alt_command": "note",
            "alias": "open_note",
            "group": "Notes",
            "description": "Add a note for today if one doesn't already exist, or edit the existing note if it does."
        },
        {
            "command": "note:list",
            "alt_command": "note:l",
            "alias": "list_notes --action:open",
            "group": "Notes",
            "description": "List of available notes, chose what to open"
        },
        {
            "command": "note:delete",
            "alt_command": "note:d",
            "alias": "list_notes --action:delete",
            "group": "Notes",
            "description": "Delete a chosen note"
        },
        {
            "command": "vsce:install",
            "alt_command": "vsce:i",
            "alias": ". $PATH_DEVAPPS/setup.sh install:vscode.ext",
            "group": "VSCode Extensions",
            "description": "Install Vscode extensions"
        },
        {
            "command": "vsce:backup",
            "alt_command": "vsce:bak",
            "alias": ". $PATH_DEVAPPS/setup.sh backup:vscode.ext",
            "group": "VSCode Extensions",
            "description": "Backup list of Vscode extensions"
        },
        {
            "command": "gitignore",
            "alias": "add_gitignore",
            "group": "git",
            "description": "Copy gitignore from git dotfiles"
        },
        {
            "command": "rc",
            "alias": "open_autoload_files",
            "group": "Shell: Files",
            "description": "Open autoload files for editing. [Run Command Files List]"
        },
        {
            "command": "to:bash",
            "alias": "sudo chsh $user -s /bin/bash && echo 'Logout now.'",
            "group": "Shell: Switch",
            "description": "Switch to bash shell"
        },
        {
            "command": "cd:add",
            "alt_command": "cd:a",
            "alias": "add_to_list_override_command_cd",
            "group": "Change Directory",
            "description": "Add current directory to cd list"
        },
        {
            "command": "cd:list",
            "alt_command": "cd:l",
            "alias": "go_to_list_override_command_cd",
            "group": "Change Directory",
            "description": "Go to a directory in cd list"
        },
        {
            "command": "cd",
            "alias": "override_command_cd",
            "group": "Change Directory",
            "description": "Go to a directory in cd list"
        },
        {
            "command": "g:add",
            "alt_command": "ga",
            "alias": "add_override_command_git",
            "group": "git",
            "description": "Git add"
        },
        {
            "command": "g:commit",
            "alt_command": "gc",
            "alias": "commit_override_command_git",
            "group": "git",
            "description": "Git commit"
        },
        {
            "command": "g:checkout",
            "alt_command": "gco",
            "alias": "git checkout",
            "group": "git",
            "description": "Git checkout"
        },
        {
            "command": "g:fp",
            "alt_command": "gfp",
            "alias": "git fetch; git pull",
            "group": "git",
            "description": "Git fetch and git pull"
        },
        {
            "command": "g:log",
            "alt_command": "gl",
            "alias": "git log --all --graph --format=oneline -n 10",
            "group": "git",
            "description": "Git log, 10 lines only"
        },
        {
            "command": "g:status",
            "alt_command": "gs",
            "alias": "status_override_command_git",
            "group": "git",
            "description": "Git status"
        },
        {
            "command": "g:rh",
            "alt_command": "grh",
            "alias": "git reset --hard",
            "group": "git",
            "description": "Git reset hard"
        },
        {
            "command": "g:push",
            "alt_command": "gpush",
            "alias": "git push origin HEAD",
            "group": "git",
            "description": "Git push to origin head"
        }
    ]
}